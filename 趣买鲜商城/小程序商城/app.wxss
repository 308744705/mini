/**app.wxss**/
page{
  background: #f5f5f5;
}
.whiteBg{
  background: #fff;
}
.body{
  background: #f5f5f5;
  padding-bottom: 110rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
view{
  font-size: 28rpx;
  line-height: 60rpx;
}
.icon{
  width: 45rpx;
  height: 45rpx;
  margin: 0 5px;
  vertical-align: middle;
}
.icon>image{
  width: 100%;
  height: 100%;
}
.flex_bet{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.flex_end{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.col_center{
  display:flex;
  align-items:center;
}
.red{
  color:#E4393C;
}
.gray{
  color: #8a8a8a;
  font-size: 26rpx;
}
.over_two{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goShop{
  color: #fff;
  font-size: 32rpx;
  width: 320rpx;
  padding: 20rpx 0;
  border-radius: 50rpx;
  background: #000;
  text-align: center;
  box-shadow: 0 6px 20px -3px rgba(0, 0, 0, .4);
}
.empty_cart{
  display: flex;
  flex-flow: column;
  align-items: center;
  justify-content: space-between;
  margin-top:80px;
  height:580rpx;
}
.empty_cart>text{
  color:#666;
  margin-top: 20px;
  font-size: 30rpx;
}
.empty_icon{
  width: 220rpx;
  height: 220rpx;
}
.empty_icon>image{
  width: 100%;
  height: 100%;
}
.arr_right{
  width: 32rpx;
  height: 32rpx;
  margin: -13rpx -5rpx 0 5rpx;
}
.arr_right>image{
  width: 100%;
  height: 100%;
}